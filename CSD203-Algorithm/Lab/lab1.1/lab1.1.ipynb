    "#Thêm từng chữ số Fibonnachi vào list/array bằng cách cộng 2 số trước đó lại\n",
    "def fibonacci(n):\n",
    "    lst = []\n",
    "    if n <=1: return n\n",
    "    lst.append(0)\n",
    "    lst.append(1)\n",
    "    for i in range (2,n+1):\n",
    "        lst.append(lst[i-2]+lst[i-1])\n",
    "    return lst[n]\n",
    "\n",
    "print(fibonacci(44))\n"
   ]
  }
